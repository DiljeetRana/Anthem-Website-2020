    <script type="text/javascript">
        $(document).ready(function () {

            $("#myProgress").fadeIn(1);
            var timerStart = Date.now();

            var elem = document.getElementById("myBar");
            var width = 10;
            var interval = Date.now() - timerStart;
            var id = setInterval(frame, interval);
            function frame() {
                if (width >= 100) {
                    clearInterval(id);
                }
                else {
                    width++;
                    elem.style.width = width + '%';
                }
            }
            var hide_Time = interval + 2000;
            $("#myProgress").fadeOut(hide_Time);

            $('.carousel').carousel({
                interval: 100
            });

            var EmailResult = $('#EmailResult').html();

            if (EmailResult == 'Email Sent') {
                alert("Email Sent");
            }

            if (EmailResult == "Email Sending Failed, Please Try Again Later") {
                alert("Sending Email Failed");
            }
        });


    </script>
