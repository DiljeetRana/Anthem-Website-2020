    <script>
        $(document).ready(function () {
            $('#btnsubmit').click(function (event) {

                data = $('.password').val();
                var len = data.length;

                if (len < 1) {
                    alert("Enter Password");
                    // Prevent form submission
                    event.preventDefault();
                }
                if ($('.password').val() != $('.confpass').val()) {
                    alert("Password and Confirm Password don't match");
                    // Prevent form submission
                    event.preventDefault();
                }

            });

            $('.dropdown a.test').on("click", function (e) {
                $(this).next('ul').toggle();
                e.stopPropagation();
                e.preventDefault();
            });
        });
    </script>
</head>
<body>
    <form id="Form1" runat="server" class="form1">
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!--<img src="Images/centa.png" class="centa">-->
            <div class="container-fluid" style="padding: 0px;">
                <div class="navbar-header" style="margin-right: 0px;">
                    <a class="img-responsive" href="login.aspx">
                        <img src="Images/anthem-infotech.jpg" alt="Anthem infotech" title="Anthem Infotech" class="img-thumbnail" width="100%" height="65" />
                    </a>
                </div>
                <div class="Media_Content">
                    <br />
                    <div style="float: left">
                        <input type="text" class="form-control" id="txtname" tabindex="1" runat="server" placeholder="Email Id(Official)" style="width: auto;" />
                    </div>
                    <div style="float: left">
                        <button class="btn btn-primary">Request OTP</button>
                    </div>
                    <div style="float:left;margin-top: 8px;">
                        <label ID="Label3" runat="server" style="color:white; margin-left:35px;" Text="">OTP not working !.</label>
                    </div>
                    <div style="float:left;margin-top: 8px;">
                        <a href="\Home\LoginNew" style="color:white; margin-left:10px; text-decoration:underline;font-weight: bold;">Login Other Way.</a>
                    </div>
                </div>
            </div>
        </nav>
        <!-- Large Screen !-->
        <div style="width: auto; height: 35px; background-color: #6886C4; border: 2px #395693 solid;" class="LargeScreenOptions">
            <div style="margin-left: 115px; float: left; border-right: 2px #ffffff groove; height: 32px; padding-right: 20px; padding-top: 3px;">
                <a href="login.aspx" style="text-decoration: none; color: white; font-family: Verdana;">Home</a>
            </div>
            <div style="margin-left: 20px; float: left; border-right: 2px #ffffff groove; height: 32px; padding-right: 20px; padding-top: 3px;">
                <a href="holidaylist2.aspx" style="text-decoration: none; color: white; font-family: Verdana;">Holidays</a>
            </div>

            <div style="margin-left: 20px; float: left; border-right: 2px #ffffff groove; height: 32px; padding-right: 20px; padding-top: 3px;">
                <a href="ourteam2.aspx" style="text-decoration: none; color: white; font-family: Verdana;">Our Team</a>
            </div>
            <div style="margin-left: 20px; float: left; border-right: 2px #ffffff groove; height: 32px; padding-right: 20px; padding-top: 3px;">
                <a href="Utilities.aspx" style="text-decoration: none; color: white; font-family: Verdana;">Utility Tools</a>
            </div>

        </div>

        <!-- Small Screen !-->
        <div style="width: auto; height: 35px; background-color: #6886C4; border: 2px #395693 solid;" class="SmallScreenOptions">
            <div class="dropdown">
                <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    Menu
                </button>
                <!--Multilevel Dropdown Starts-->
                <ul class="dropdown-menu">
                    <li><a class="dropdown-item" tabindex="-1" href="login.aspx">Home</a></li>
                    <li><a class="dropdown-item" tabindex="-1" href="holidaylist2.aspx">Holidays</a></li>
                    <li><a class="dropdown-item" tabindex="-1" href="ourteam2.aspx">Our Team</a></li>
                    <li><a class="dropdown-item" tabindex="-1" href="Utilities.aspx">Utility Tools</a></li>

                </ul>
                <!--Multilevel Dropdown Ends-->
            </div>
        </div>

        <asp:ScriptManager runat="server"></asp:ScriptManager>
        <ajaxToolkit:ModalPopupExtender ID="mdlpopup"
                                        BackgroundCssClass="background"
                                        PopupControlID="pshow"
                                        TargetControlID="btnlogin"
                                        DropShadow="true" runat="server">
        </ajaxToolkit:ModalPopupExtender>

        <div>
            @RenderBody()
        </div>
    </form>
    <div style="clear: both">
