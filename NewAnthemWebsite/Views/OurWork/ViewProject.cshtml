    <script>
        var onloadCallback = function () {
            grecaptcha.render('dvCaptcha', {
                'sitekey': '6LdansIZAAAAABFu1RU10hD3CCs1HN94mpjPOoga',
                'callback': function (response) {
                    $.ajax({
                        type: "POST",
                        url: "/Career/AjaxMethod",
                        data: "{response: '" + response + "'}",
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (r) {
                            var captchaResponse = jQuery.parseJSON(r.Response);
                            if (captchaResponse.success) {
                                $("#hfCaptcha").val(captchaResponse.success);
                                $("#rfvCaptcha").css('display', 'none')
                            } else {
                                $("#hfCaptcha").val("");
                                $("#rfvCaptcha").css('display', 'block')
                                var error = captchaResponse["error-codes"][0];
                                $("#rfvCaptcha").html("RECaptcha error. " + error);
                            }
                        }
                    });
                }
            });
        };
    </script>

